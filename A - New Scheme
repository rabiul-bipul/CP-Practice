#include <iostream>
using namespace std;

int main()
{
    int arr[8], fg = 0;
    for (int i = 0; i < 8; i++)
    {
        scanf("%d", &arr[i]);
    }
    for (int i = 0; i < 8; i++)
    {
        if ((i < 7) && (arr[i] > arr[i + 1]))
        {
            printf("No");
            return 0;
        }
        if (arr[i] < 100 || arr[i] > 675 || arr[i] % 25 != 0)
        {
            printf("No");
            return 0;
        }
    }

    printf("Yes");

    return 0;
}
