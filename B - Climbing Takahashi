#include <iostream>
using namespace std;

int main()
{
    int n;
    scanf("%d", &n);
    int arr[n];
    for (int i = 0; i < n; i++)
    {
        scanf("%d", &arr[i]);
    }
    for (int i = 0; i < n; i++)
    {
        if (i != n - 1 && arr[i] < arr[i + 1])
        {
            continue;
        }
        else
        {
            printf("%d", arr[i]);
            return 0;
        }
    }

    return 0;
}
