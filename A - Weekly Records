#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    scanf("%d", &n);

    for (int i = 0; i < n; i++)
    {
        int arr[7];
        for (int j = 0; j < 7; j++)
        {
            scanf("%d", &arr[j]);
        }
        int ans = 0;
        for (int j = 0; j < 7; j++)
        {
            ans += arr[j];
        }
        printf("%d ", ans);
    }
    printf("\n");
}
