#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n, m, sum = 0;
    scanf("%d %d", &n, &m);
    int arr[n];

    for (int i = 0; i < n; i++)
    {
        scanf("%d", &arr[i]);
    }
    for (int i = 0; i < m; i++)
    {
        int num;
        scanf("%d", &num);
        sum += arr[num - 1];
    }

    printf("%d", sum);
}
